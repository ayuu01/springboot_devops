
pipeline {
    agent any
    
    stages {
        stage('Build project') {
            steps {
               bat 'mvn clean -f SpringBootDevops/pom.xml'
            }
        }
        stage('Build Docker Image') {
            steps {
               script {
                   bat 'docker build -t devops/devops-assignment -f SpringBootDevops/Dockerfile .'
               }
            }
        }
       
    }
}
